@model IEnumerable<Students>

<table>
    <tr>
        <th>Name</th>
       @*  <th>Class</th> *@
        <th>Date Of Birth</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
        @*     <td>@item.classes.ClassName</td> *@
            <td>@item.DOB</td>
        </tr>
    }
</table>